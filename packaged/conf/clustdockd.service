# To be put at: /usr/lib/systemd/system/clustdock-server.service

[Unit]
Description=Service for spawning VMs and Docker containers
Requires=network-online.target
After=network-online.target

[Service]
ExecStart=/usr/bin/clustdockd -c /etc/clustdockd.conf -d /etc/clustdock.dump.bin -l DEBUG --logfile /var/log/clustdockd.log

[Install]
WantedBy=multi-user.target
